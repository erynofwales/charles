# SConscript
# vim: set ft=python:
#
# SConscript for building tests for charles.
#
# Eryn Wells <eryn@erynwells.me>

Import('env')
Import('gtest')
Import('charles_lib')

files = Split("""
    test_asserts.c
    test_basics.c
    test_charles.c
    test_object.c
""")

prog = env.Program('test_charles', [gtest, charles_lib, files])
env.Alias('test', prog)
