# SConscript
# vim: set ft=python:
#
# SConscript for building tests for charles.
#
# Eryn Wells <eryn@erynwells.me>

Import('env')

files = Split("""
    test_basics.c
    test_charles.c
""")

test_env = env.Clone()
test_env.Append(LIBS=['check'])
lib = test_env.Library('test_charles', files, libs=['check'])
Return('lib')
