Import('env')

files = Split("""
    basics.cc
    object.cc
    object_sphere.cc
    scene.cc
    writer_png.cc
""")

lib = env.Library('charles', files)
prog = env.Program('charles', [lib, 'charles.cc'])
env.Alias('charles', prog)

Return('lib')
