Import('env')


files = [
    'cameraParser.cc',
    'objectParser.cc',
    'parsers.cc',
    'scalarMappingParser.cc',
    'sceneParser.cc',
    'vectorParser.cc'
]

objs = []
for f in files:
    objs.append(env.Object(f))

Return('objs')
